<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FurkanPayerFinalProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FurkanPayerFinalProject"
    xmlns:pages="clr-namespace:FurkanPayerFinalProject.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="FurkanPayerFinalProject">

    <ShellContent Title="Login" ContentTemplate="{DataTemplate pages:LoginPage}" Route="LoginPage" />
    <ShellContent Title="Register" ContentTemplate="{DataTemplate pages:RegisterPage}" Route="RegisterPage" />
    <ShellContent Title="Criteria" ContentTemplate="{DataTemplate pages:CriteriaPage}" Route="CriteriaPage" />
    <ShellContent Title="Final Details" ContentTemplate="{DataTemplate pages:FinalDetailsPage}" Route="FinalDetailsPage" />
</Shell>
